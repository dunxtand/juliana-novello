<head>
  <meta charset="utf-8">

  {# force Internet Explorer to use the latest rendering engine available #}
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title>
    {% if wp_title %}
      {{ wp_title }} - {{ site.name }}
    {% else %}
      {{ site.name }}
    {% endif %}
  </title>

  {# ldFriendly" content="True"> #}
  <meta name="MobileOptimized" content="320">
  <meta name="viewport" content="width=device-width, initial-scale=1"/>

  {# preload fonts for lighthouse audit #}
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/GTWalsheimPro.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/GTWalsheimPro-Light.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/GTWalsheimPro-Medium.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/GTWalsheimPro-Bold.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/Canela-Thin.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/Canela-Light.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/Canela-Bold-Web.woff" as="font" crossorigin="anonymous" />
  <link rel="preload" href="../../wp-content/themes/Tailbase/dist/fonts/ATAura.woff" as="font" crossorigin="anonymous" />

  <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/dist/images/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ theme.link }}/dist/images/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ theme.link }}/dist/images/favicon-16x16.png">
  <link rel="manifest" href="{{ theme.link }}/dist/images/site.webmanifest">
  <link rel="mask-icon" href="{{ theme.link }}/dist/images/safari-pinned-tab.svg" color="#666666">
  <meta name="msapplication-TileColor" content="#000000">
  <meta name="theme-color" content="#ffffff">
  <meta name="description" content="{{ site.description }}">
  <!--[if IE]>
  <link rel="shortcut icon" href="">
  <![endif]-->

  <link rel="pingback" href="{{ site.pingback_url }}"/>

  {{ function('wp_head') }}

  <!-- Google Analytics -->
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-89636583-1', 'auto');
  ga('send', 'pageview');
  </script>
  <!-- End Google Analytics -->
  
</head>
